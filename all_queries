PUT dynamic_temp
{
  "mappings":{
        
    "dynamic_templates":[
        {
            "long_to_strings" :{
              
              "match_mapping_type":"long",
              "match": "first",
              
              "path_match": "name.*",
              "path_unmatch": "*.middle",
              
              "mapping":{
                "type": "text",
                "copy_to": "full_name",
                "fields":{
                  "raw": {
                    "type": "keyword"
                  }
                }
                
              }
              
              
              
            }
        }
      
      ]

    
  }
  
}

