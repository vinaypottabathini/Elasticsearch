POST runt_idx/_search
{
  
  "runtime_mappings": {
    "measures.voltage": {
      "type": "double",
      "script": {
        "source": 
        """if (doc['model_number.keyword'].value.equals('HG537PU'))
          { emit(1.7 * params._source['measures']['voltage']); }
          else{emit(params._source['measures']['voltage']);}
        """
      }
    }
  }, 
  
  "query": {
    "match": {
      "model_number": "HG537PU"
    }
  },
  "fields": ["measures.voltage"]
  
}





