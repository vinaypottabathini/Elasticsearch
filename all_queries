            "type": "text",
            "analyzer": "standard",
            "fields":{
              "raw":{
                
                "type": "text",
                "analyzer": "{name}"
                
              }
              
            }
            
          }
          
        }
      },
      
      {
        "no_doc_values":{
          
          "match_mapping_type": "*",
          "mapping":{
            
            "type": "{dynamic_type}",
            "doc_values": false
          }
          
          
        }
      }
    ]
 
  }
  
