      "type": "long"
    }
    
    
    
  }
  
  
}


GET runt_idx/_search
{
  "aggs":{
    
    "avg_ms_starts":{
      "avg":{
        "field": "measures.start"
      }
      
  },
  
  "avg_ms_ends":{
      "avg":{
        "field": "measures.end"
      }
      
    }
  
}

}



GET runt_idx/_search
{
  "runtime_mappings": {
    "duration": {
      "type": "long",
      "script": {
        "source": """
          emit(doc['measures.end'].value - doc['measures.start'].value );
        """
        
      }
    }
  },
  
  "aggs":{
    
    "duration_stats":{
      "stats":{
        "field": "duration"
      }
    }
    
  }
  
  
