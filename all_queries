  "is_published": false
}


GET boolean/_search
{
  
  "aggs":{
    
    "publish_state":{
      "terms":
        {
          "field": "is_published"
        }
    }
    
  },
  "sort": ["is_published"],
  "fields": [ "weight"
      
    ],
  "runtime_mappings": {
    "weight": {
      "type": "long",
      "script": "emit(doc['is_published'].value? 10: 0)"
    }
  }
  
  
}

