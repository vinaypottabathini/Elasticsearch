GET my-index-000001/_search
{
  "sort": {"date": "asc"},
  "runtime_mappings": {
    "date_has_nanos": {
      "type": "boolean",
      "script": "emit(doc['date'].value.nano!=0)"
    }
  },
  "fields": [
    {
      "field": "date",
