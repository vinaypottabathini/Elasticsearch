PUT dynamic_temp
{
  "mappings": {
    "dynamic_templates": [ 
      
      {
        "string_as_ip":{
          "match_mapping_type": "string",
          "match": "ip*",
          "runtime":{
            "type": "ip"
          }
        }
      
      }
    
    ]
  }
}


